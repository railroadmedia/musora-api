{
  "name": "railroad/musora-api",
  "type": "library",
  "license": "MIT",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "railroad/railcontent": "2.1.*",
    "railroad/permissions": "^2.0",
    "railroad/response": "2.0.x-dev",
    "intervention/image": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "phpunit/php-code-coverage": "^9.2.10",
    "orchestra/testbench": "^7.4",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\MusoraApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\MusoraApi\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "composer/package-versions-deprecated": true
    }
  }
}
